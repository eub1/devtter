{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": [
		"next/core-web-vitals",
		"next/babel",
		"plugin:react/recommended",
		"standard",
		"prettier"
	],
	"overrides": [],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"react/no-unescaped-entities": "off",
		"@next/next/no-page-custom-font": "off",
		"react/prop-types": "off"
	}
}
